<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.InnerShadow?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox spacing="10.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.fxmlfilescontroller.NewMember">
   <children>
      <HBox alignment="CENTER" prefHeight="60.0" prefWidth="660.0" spacing="10.0">
         <children>
            <Label alignment="CENTER" contentDisplay="CENTER" text="New Member" textAlignment="CENTER">
               <font>
                  <Font name="Copperplate Gothic Bold" size="36.0" />
               </font>
            </Label>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </HBox>
      <ScrollPane>
         <content>
            <VBox>
               <children>
                  <HBox alignment="CENTER_LEFT" spacing="10.0">
                     <children>
                        <VBox>
                           <children>
                              <HBox alignment="CENTER_LEFT" spacing="10.0">
                                 <children>
                                    <Label alignment="CENTER" prefWidth="120.0" text="Member ID">
                                       <font>
                                          <Font size="13.0" />
                                       </font>
                                    </Label>
                                    <TextField fx:id="memberid" prefHeight="25.0" prefWidth="180.0" />
                                 </children>
                                 <padding>
                                    <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
                                 </padding>
                              </HBox>
                              <HBox alignment="CENTER_LEFT" spacing="10.0">
                                 <children>
                                    <Label alignment="CENTER" prefWidth="120.0" text="First Name">
                                       <font>
                                          <Font size="13.0" />
                                       </font>
                                    </Label>
                                    <TextField id="firstname" fx:id="fname" prefHeight="25.0" prefWidth="180.0" />
                                 </children>
                                 <padding>
                                    <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
                                 </padding>
                              </HBox>
                              <HBox alignment="CENTER_LEFT" spacing="10.0">
                                 <children>
                                    <Label alignment="CENTER" prefWidth="120.0" text="Middle Name">
                                       <font>
                                          <Font size="13.0" />
                                       </font>
                                    </Label>
                                    <TextField id="middlename" fx:id="mname" prefHeight="25.0" prefWidth="180.0" />
                                 </children>
                                 <padding>
                                    <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
                                 </padding>
                              </HBox>
                              <HBox alignment="CENTER_LEFT" spacing="10.0">
                                 <children>
                                    <Label alignment="CENTER" prefWidth="120.0" text="Last Name">
                                       <font>
                                          <Font size="13.0" />
                                       </font>
                                    </Label>
                                    <TextField id="lastname" fx:id="lname" prefHeight="25.0" prefWidth="180.0" />
                                 </children>
                                 <padding>
                                    <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
                                 </padding>
                              </HBox>
                              <HBox alignment="CENTER_LEFT" spacing="10.0">
                                 <children>
                                    <Label alignment="CENTER" prefWidth="120.0" text="Gender">
                                       <font>
                                          <Font size="13.0" />
                                       </font>
                                    </Label>
                                    <ChoiceBox id="gender" fx:id="gender" prefWidth="150.0" />
                                 </children>
                                 <padding>
                                    <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
                                 </padding>
                              </HBox>
                              <HBox alignment="CENTER_LEFT" spacing="10.0">
                                 <children>
                                    <Label alignment="CENTER" prefWidth="120.0" text="D.O.B.">
                                       <font>
                                          <Font size="13.0" />
                                       </font>
                                    </Label>
                                    <DatePicker id="dateofbirth" fx:id="dob" prefHeight="25.0" prefWidth="129.0" />
                                 </children>
                                 <padding>
                                    <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
                                 </padding>
                              </HBox>
                              <HBox alignment="CENTER_LEFT" spacing="10.0">
                                 <children>
                                    <Label alignment="CENTER" prefWidth="120.0" text="Mobile no.">
                                       <font>
                                          <Font size="13.0" />
                                       </font>
                                    </Label>
                                    <TextField id="mobile_no" fx:id="mobileno" prefWidth="180.0" />
                                 </children>
                                 <padding>
                                    <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
                                 </padding>
                              </HBox>
                              <HBox alignment="CENTER_LEFT" spacing="10.0">
                                 <children>
                                    <Label alignment="CENTER" prefWidth="120.0" text="email id">
                                       <font>
                                          <Font size="13.0" />
                                       </font>
                                    </Label>
                                    <TextField id="email" fx:id="email" prefHeight="25.0" prefWidth="180.0" />
                                 </children>
                                 <padding>
                                    <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
                                 </padding>
                              </HBox>
                              <HBox alignment="CENTER_LEFT" spacing="10.0">
                                 <children>
                                    <Label alignment="CENTER" prefWidth="120.0" text="College/Institute">
                                       <font>
                                          <Font size="13.0" />
                                       </font>
                                    </Label>
                                    <ChoiceBox id="college" fx:id="college" prefWidth="150.0" />
                                 </children>
                                 <padding>
                                    <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
                                 </padding>
                              </HBox>
                              <HBox alignment="CENTER_LEFT" spacing="10.0">
                                 <children>
                                    <Label alignment="CENTER" prefWidth="120.0" text="Branch/Course">
                                       <font>
                                          <Font size="13.0" />
                                       </font>
                                    </Label>
                                    <ChoiceBox id="branch" fx:id="branch" prefWidth="150.0" />
                                 </children>
                                 <padding>
                                    <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
                                 </padding>
                              </HBox>
                              <HBox alignment="TOP_CENTER" spacing="10.0">
                                 <children>
                                    <Label alignment="CENTER" prefWidth="120.0" text="Address">
                                       <font>
                                          <Font size="13.0" />
                                       </font>
                                    </Label>
                                    <TextArea fx:id="address" prefHeight="89.0" prefWidth="237.0" />
                                 </children>
                                 <padding>
                                    <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
                                 </padding>
                              </HBox>
                           </children>
                        </VBox>
                        <VBox alignment="TOP_CENTER" spacing="10.0">
                           <children>
                              <ImageView fx:id="profilePicture" fitHeight="192.0" fitWidth="180.0" pickOnBounds="true" preserveRatio="true">
                                 <effect>
                                    <InnerShadow color="#756767" />
                                 </effect>
                              </ImageView>
                              <Button fx:id="upload" mnemonicParsing="false" onAction="#uploadPicture" text="Upload Photo" />
                           </children>
                           <padding>
                              <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                           </padding>
                        </VBox>
                     </children>
                     <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" />
                     </padding>
                  </HBox>
                  <Label fx:id="status" alignment="CENTER" prefHeight="25.0" prefWidth="621.0" textAlignment="CENTER" />
               </children>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
            </VBox>
         </content>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </ScrollPane>
      <HBox alignment="CENTER_RIGHT" spacing="10.0">
         <children>
            <Button fx:id="Submit" alignment="CENTER" mnemonicParsing="false" onAction="#submit" prefWidth="120.0" text="Submit">
               <font>
                  <Font size="13.0" />
               </font>
            </Button>
            <Button fx:id="erase" alignment="CENTER" mnemonicParsing="false" prefWidth="120.0" text="Reset">
               <font>
                  <Font size="13.0" />
               </font>
            </Button>
            <Button fx:id="back" alignment="CENTER" mnemonicParsing="false" prefWidth="120.0" text="Close">
               <font>
                  <Font size="13.0" />
               </font>
            </Button>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </HBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
