            <VBox prefWidth="679.0">
               <children>
                  <HBox prefHeight="360.0" prefWidth="592.0" spacing="10.0">
                     <children>
                        <VBox prefHeight="340.0" prefWidth="350.0" HBox.hgrow="ALWAYS">
                           <children>
                              <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0" spacing="10.0">
                                 <children>
                                    <Label prefWidth="90.0" text="Member Id : " />
                                    <TextField fx:id="memberId" />
