<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="TOP_CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="10.0" style="-fx-border-color: grey; -fx-border-radius: 15;" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.fxmlfilescontroller.BookDetails">
   <children>
      <TableView fx:id="bookTable" tableMenuButtonVisible="true" VBox.vgrow="ALWAYS">
        <columns>
          <TableColumn fx:id="bookId" prefWidth="75.0" text="Book Id" />
          <TableColumn fx:id="bookName" prefWidth="75.0" text="Book Name" />
            <TableColumn fx:id="author" prefWidth="75.0" text="Author" />
            <TableColumn fx:id="publisher" prefWidth="75.0" text="Publisher" />
            <TableColumn fx:id="category" maxWidth="-1.0" minWidth="-1.0" prefWidth="-1.0" text="Category" />
            <TableColumn fx:id="totalCopy" prefWidth="75.0" text="Total Books" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
