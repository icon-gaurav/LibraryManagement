<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="TOP_CENTER" spacing="10.0" style="-fx-border-radius: 15; -fx-border-color: grey;" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.fxmlfilescontroller.MemberDetails">
   <children>
      <TableView fx:id="memberdetails" cache="true" style="-fx-border-radius: 15;" tableMenuButtonVisible="true" VBox.vgrow="ALWAYS">
        <columns>
          <TableColumn fx:id="memberId" prefWidth="75.0" text="Member ID" />
          <TableColumn fx:id="memberName" prefWidth="75.0" text="Name" />
            <TableColumn fx:id="dob" prefWidth="75.0" text="D.O.B." />
            <TableColumn fx:id="gender" prefWidth="75.0" text="Gender" />
            <TableColumn fx:id="address" prefWidth="75.0" text="Address" />
            <TableColumn fx:id="joinDate" prefWidth="75.0" text="Joining Date" />
            <TableColumn fx:id="bookIssued" prefWidth="75.0" text="Book Issued" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
